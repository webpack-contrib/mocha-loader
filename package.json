{
  "name": "mocha-loader",
  "version": "2.0.0",
  "description": "Mocha loader for webpack",
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "engines": {
    "node": ">= 6"
  },
  "scripts": {
    "lint": "eslint --cache src test",
