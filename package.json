  "devDependencies": {
    "@webpack-contrib/defaults": "^3.0.5",
    "@webpack-contrib/eslint-config-webpack": "^3.0.0",
    "@webpack-contrib/test-utils": "^0.1.3",
    "eslint": "^5.14.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.1.0",
    "mocha": "^6.0.1",
    "prettier": "^1.16.4",
    "standard-version": "^5.0.0",
    "webpack": "^4.29.5"
