  "devDependencies": {
    "@webpack-contrib/eslint-config-webpack": "^3.0.0",
    "@webpack-contrib/test-utils": "^0.1.3",
    "eslint": "^5.10.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "jest": "^23.6.0",
    "mocha": "^5.2.0",
    "prettier": "^1.15.3",
    "standard-version": "^4.4.0",
    "webpack": "^4.27.1",
    "webpack-defaults": "^3.0.0"
